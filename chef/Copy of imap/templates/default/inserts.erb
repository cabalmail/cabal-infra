<% for tld in @domains.keys.sort do %>
insert into domain (name, parent_id) values('<%= tld %>', null);
<% tldobj = @domains[tld] %>
<% for subd in tldobj['subdomains'].keys.sort do %>
insert into domain (name, parent_id) select '<%= subd %>', id from domain where name='<%= tld %>';
<% end %>
<% end %>
