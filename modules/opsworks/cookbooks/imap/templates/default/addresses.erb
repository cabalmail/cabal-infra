<% for tld in @domains.keys.sort do %>
  <% tldobj = @domains[tld] %>
  <% for subd in tldobj['subdomains'].keys.sort do %>
    <% subdobj = tldobj['subdomains'][subd] %>
    <% for add in subdobj['addresses'] do %>
insert into address (address, domain_id) select '<%= add.first %>', id from domain where name = '<%= subd %>' and parent_id = (select id from domain where name = '<%= tld %>');
    <% end %>
  <% end %>
<% end %>
