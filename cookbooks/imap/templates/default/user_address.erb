<% xref = {"jake" => 1, "chris" => 2, "cathy" => 3, "tristan" => 4} %>
<% for tld in @domains.keys.sort do %>
  <% tldobj = @domains[tld] %>
  <% for subd in tldobj['subdomains'].keys.sort do %>
    <% subdobj = tldobj['subdomains'][subd] %>
    <% for add in subdobj['addresses'].keys do %>
      <% u = subdobj['addresses'][add] %>
insert into user_address (user_id, address_id) select <%= xref[u] %>, id from address where address = '<%= add %>' and domain_id = (select d.id from domain d join domain d2 on d.parent_id = d2.id where d.name = '<%= subd %>' and d2.name = '<%= tld %>');
    <% end %>
  <% end %>
<% end %>
